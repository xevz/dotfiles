(xbindkey '(control shift t) "urxvt")
; (xbindkey '(control shift f) "firefox")
; (xbindkey '(control shift f) "opera")
(xbindkey '(control shift f) "x-www-browser")
(xbindkey '(control shift g) "gvim")

(xbindkey '(control shift a) "audio_volume increase")
(xbindkey '(control shift o) "audio_volume decrease")

(xbindkey '(XF86AudioRaiseVolume) "audio_volume increase")
(xbindkey '(XF86AudioLowerVolume) "audio_volume decrease")
(xbindkey '(XF86AudioMute) "audio_volume mute")

(xbindkey '(control shift m) "mpc prev")
(xbindkey '(control shift w) "mpc toggle")
(xbindkey '(control shift v) "mpc stop")
(xbindkey '(control shift z) "mpc next")

(xbindkey '(mod5 "c:10") "warppointer 1")
(xbindkey '(mod5 "c:11") "warppointer 0")
(xbindkey '(mod5 "c:12") "warppointer 2")
(xbindkey '(mod5 "c:13") "warppointer 3")

(xbindkey '(shift Print) "scrot -e 'mv $f ~/.screenshots'")

(xbindkey '(shift mod1 p) "passmenu --type")

